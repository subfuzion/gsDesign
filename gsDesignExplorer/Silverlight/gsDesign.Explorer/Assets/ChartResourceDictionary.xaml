				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="toolkit:CategoryAxis">
					<Grid x:Name="AxisGrid" Background="{TemplateBinding Background}">
						<toolkit:Title x:Name="AxisTitle" Style="{TemplateBinding TitleStyle}"/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
