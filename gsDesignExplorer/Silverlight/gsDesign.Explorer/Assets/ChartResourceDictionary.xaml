				<Style TargetType="Line">
					<Setter Property="Stroke" Value="Black"/>
					<Setter Property="X2" Value="4"/>
					<Setter Property="Y2" Value="4"/>
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="GridLineStyle">
