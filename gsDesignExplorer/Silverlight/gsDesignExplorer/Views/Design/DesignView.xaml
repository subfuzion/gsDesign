<UserControl x:Class="gsDesign.Explorer.Views.Design.DesignView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:Design="clr-namespace:gsDesign.Explorer.Views.Design"
	mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="White">
		<sdk:TabControl x:Name="DesignTabControl">
			<sdk:TabItem Header="a * Power * Timing">
				<Design:EPTView />
			</sdk:TabItem>
			<sdk:TabItem Header="Sample Size">
				<Design:SampleSizeView />
			</sdk:TabItem>
			<sdk:TabItem Header="Spending Functions">
				<Design:SpendingFunctionsView />
			</sdk:TabItem>
		</sdk:TabControl>
	</Grid>
</UserControl>
